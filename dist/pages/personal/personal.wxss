.weui-navbar {
display:-webkit-box;
display:-webkit-flex;
display:flex;
position:absolute;
z-index:500;
background-color: #fff;
top:0;
width:100%;

}
.button_css{
  width:160rpx;height:55rpx;font-size:24rpx;background-color:#fff;border: 1px solid #999;
  position: relative;
  top: -4rpx;
}
button[type="default"] {
color:#000000;
background-color:#fff;

}
button::after {
content:" ";
width:200%;
height:200%;
position:absolute;
top:0;
left:0;
border:0px solid rgba(0, 0, 0, 0.2);
-webkit-transform:scale(0.5);
transform:scale(0.5);
-webkit-transform-origin:0 0;
transform-origin:0 0;
box-sizing:border-box;
border-radius:10px;
}

.weui-tab__panel {
  box-sizing:border-box;
  height:49%;
  padding-top:50px;
  overflow:auto;
  padding-bottom:57rpx; 
  -webkit-overflow-scrolling:touch;
  background-color: #E8EFEF;
}

.weui-navbar__slider {
  position:absolute;
  content:" ";
  left:0;
  bottom:0;
  width:3em;
  height:8rpx;
  background-color:#1aad19;
  -webkit-transition:-webkit-transform .3s;
  transition:-webkit-transform .3s;
  transition:transform .3s;
  transition:transform .3s,-webkit-transform .3s;

}

