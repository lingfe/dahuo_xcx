<view class="page">
    <view class="">
        <view class="margin_left">
            <!-- 头像  start -->
            <view class="weui-cell_to weui-cells_to title" bindtap="bindtapUpdateName">
                <view class="weui-cell__bd">头像</view>
                <view class="weui-cell__ft weui-cell__ft_in-access">
                  <view >
                    <image class="img_title" src="http://img1.3lian.com/gif/more/11/201212/cd1d745ed855bef27f47c8aff0786067.jpg"/>
                  </view>
                </view>
            </view>
            <!-- 头像  end -->

            <!-- 姓名  start -->
            <view class="weui-cell_to weui-cells_to title" bindtap="bindtapUpdateName">
                <view class="weui-cell__bd">姓名</view>
                <view class="weui-cell__ft weui-cell__ft_in-access">{{updateName  == null ? '请填写' : updateName}}</view>
            </view>
            <!-- 姓名  end -->

            <!-- 地域  start -->
            <view class="weui-cell_to weui-cells_to weui-cells_after-title title" bindtap="bindtapSearch">
                <view class="weui-cell__bd">地域</view>
                <view class="weui-cell__ft weui-cell__ft_in-access">贵阳</view>
            </view>
            <!-- 地域  end -->

            <!-- 签名  start -->
            <view class="weui-cell_to weui-cells_to title"  bindtap="bindtapAutograph">
                <view class="weui-cell__bd">签名</view>
                <view class="weui-cell__ft weui-cell__ft_in-access">{{autograph  == null ? '请填写' : autograph}}我就是我,不一样萤火</view>
            </view>
            <!-- 签名  end -->

            <!-- 手机号  start -->
            <view class="weui-cell_to weui-cells_to weui-cells_after-title title" style="border:none;" bindtap="bindtapPhone">
                <view class="weui-cell__bd">手机号</view>
                <view class="weui-cell__ft weui-cell__ft_in-access">130*******125</view>
            </view>
            <!-- 手机号  end -->
        </view>
    </view>       
</view>