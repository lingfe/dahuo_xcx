/* 弹窗 */
.jubao_bg{
  background-color:rgba(0, 0, 0, 0.5);
  width: 100%;
  position: fixed;
  z-index: 550;
  height: 100%;
  top: -1rpx;
}
.jubao{
  font-size:20rpx;
  font-family:"黑体";
  width:410rpx;
  height:auto;
  padding:42rpx;
  position:fixed;
  top:120rpx;
  left:120rpx;
  background-color:#fff;
  box-shadow:1px 1px 10px #aaa;
  float:left;
  border-radius:36rpx;
  z-index: 600;
  text-align: center;
}
.jubao_title{
  color:#14bac8;
