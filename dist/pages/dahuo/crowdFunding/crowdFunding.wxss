 page{
   background-color: #e8efef;
 }
 
 .neiro{
   width: 100%;
   height: 100%;
 }

 .neiro_model{
   width: 96%;
   margin: auto;
   height: auto;
   background-color: #fff;
   margin-top: 20rpx;
   padding-bottom:10rpx;
 }

 .model_img{
   width: 100%;
   height: auto;
   padding-top: 0rpx;
   padding-bottom: 10rpx;
 }
 .img{
   width: 100%;
   height: 400rpx;
 }
 .model_title{
   width: 100%;
   height: 50rpx;
   line-height: 50rpx;
   text-align: center;
   font-size: 34rpx;
     overflow: hidden; /**自动隐藏文字*/
  text-overflow: ellipsis;/**文字隐藏后添加省略号*/
  white-space: nowrap;/**强制不换行*/
 }
 .model_content{
   width: 90%;
   margin: auto;
   height: auto;
   max-height: 80rpx;
   line-height: 40rpx;
   font-size: 26rpx;
   color: #AAA;
   text-align: center;
     overflow: hidden; /**自动隐藏文字*/
  /* text-overflow: ellipsis;/**文字隐藏后添加省略号*/
  /* white-space: nowrap;*强制不换行 */ 
  padding-bottom: 10rpx;
 }
 .model_style{
   width: 60%;
  height: 50rpx;
  margin: auto;
  line-height: 50rpx;
  text-align: center;
  padding-bottom: 10rpx;
 }
 .data_biaoqian_view{
  font-size: 22rpx;
  float: left;
  color: #AAA;
  width: 30%;
  overflow: hidden; /**自动隐藏文字*/
  text-overflow: ellipsis;/**文字隐藏后添加省略号*/
  white-space: nowrap;/**强制不换行*/
}
.data_biaoqian_view image{
  height: 22rpx;
  width: 22rpx;
  position: relative;
  top: 3rpx;
}

/*链接*/
.navigator:active {
  background-color:#fff;/*rgba(0, 0, 0, 0.1);*/
  opacity:0.7;
}
.navigator {
  height:auto;
  width:auto;
  display:block;
}
