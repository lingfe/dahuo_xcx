<view class="page">
    <view class="">
        <view class="weui-cells_to weui-cells_after-title">
            <!-- 合伙创业  start -->
            <view class="weui-cell weui-cells" bindtap="bindtapPartnership">
                <view class="weui-cell__hd">
                    <image src="/assets/images/ico/partnership2.png" 
                    class="ico_image"></image>
                </view>
                <view class="weui-cell__bd title">合伙创业</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <!-- 合伙创业  end -->

            <!-- 生意转让  start -->
            <view class="weui-cell weui-cells weui-cells_after-title" bindtap="bindtapBusinessTransfer">
                <view class="weui-cell__hd">
                    <image src="/assets/images/ico/businessTransfer2.png" 
                    class="ico_image"></image>
                </view>
                <view class="weui-cell__bd title">生意转让</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <!-- 生意转让  end -->

            <!-- 加盟分店  start -->
            <view class="weui-cell weui-cells weui-cells_after-title" bindtap="bindtapAffiliateStores">
                <view class="weui-cell__hd">
                    <image src="/assets/images/ico/affiliateStores2.png" 
                    class="ico_image"></image>
                </view>
                <view class="weui-cell__bd title">加盟分店</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <!-- 加盟分店   end -->

            <!-- 干股纳才  start -->
            <view class="weui-cell weui-cells weui-cells_after-title" bindtap="bindtapGanguSatisfiedBefore">
                <view class="weui-cell__hd">
                    <image src="/assets/images/ico/ganguSatisfiedBefore2.png" 
                    class="ico_image"></image>
                </view>
                <view class="weui-cell__bd title">干股纳才</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <!-- 干股纳才  end -->
            
            <!-- 金融理财  start -->
            <view class="weui-cell weui-cells weui-cells_after-title" bindtap="bindtapFinancialManagement">
                <view class="weui-cell__hd">
                    <image src="/assets/images/ico/financialManagement2.png" 
                    class="ico_image"></image>
                </view>
                <view class="weui-cell__bd title">金融理财</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <!-- 金融理财  end -->
            
            <!-- 房产投资  start -->
            <view class="weui-cell weui-cells weui-cells_after-title" bindtap="bindtapPropertyInvestment">
                <view class="weui-cell__hd">
                    <image src="/assets/images/ico/propertyInvestment2.png" 
                    class="ico_image"></image>
                </view>
                <view class="weui-cell__bd title">房产投资</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <!-- 房产投资  end -->

            <!-- 微商代理  start -->
            <view class="weui-cell weui-cells weui-cells_after-title" bindtap="bindtapDerivativeAgent">
                <view class="weui-cell__hd">
                    <image src="/assets/images/ico/aerivativeAgent2.png" 
                   class="ico_image"></image>
                </view>
                <view class="weui-cell__bd title">微商代理</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <!-- 微商代理  end -->
            
            <!-- 其他  start -->
            <view class="weui-cell weui-cells_to weui-cells_after-title" bindtap="bindtapOther">
                <view class="weui-cell__hd">
                    <image src="/assets/images/ico/other2.png" 
                    class="ico_image"></image>
                </view>
                <view class="weui-cell__bd title">其他</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <!-- 其他  end -->
        </view>
    </view>
</view>