<!-- 筛选   start-->
<view  style="display: {{screen == false ? 'none' : 'block' }};" class="page pageScreen">
    <view  class="page_bd page__bdScreen">
        <view class="weui-tab">
            <!-- tab菜单  start -->
            <view class="www">
                <block wx:for="{{tabs}}" wx:key="{{*this}}">
                    <view id="{{index}}"  class="weui-navbar__item {{activeIndex == index ? ' border_tab' : 'stop_title'}}" bindtap="tabClick">
                        <view class="weui-navbar__title stop_1">{{item.name}}</view>
                    </view>
                </block>
             </view>
             <!-- tab菜单  end -->
            <view class="ccc">
                <!-- 金额  start -->
                <view class="" hidden="{{activeIndex != 0}}">                
                  <checkbox-group bindchange="checkboxChange" data-index="{{activeIndex}}">
                  <label  wx:for="{{tabs[activeIndex].content}}"  wx:key="{{*this}}">
                    <view class="weui-navbar__item stop">
                    <checkbox class="weui-check" value="{{item.value}}" checked="{{item.checked}}"/>
                    <view class="weui-navbar__title stop_1 flaot_left">{{item.minThreshold}} - {{item.maxThreshold}} 万</view>
                    <view class="flaot_right"  wx:if="{{item.checked}}">
                        <icon class="weui-icon-radio" type="success_no_circle" color="#14BAC8" size="16"></icon>
                    </view>
                    </view>
                  </label>
                  </checkbox-group>
                </view>
                <!-- 金额  end -->

                <!-- 行业  start -->
                <view class="" hidden="{{activeIndex != 1}}">                
                  <checkbox-group  bindchange="checkboxChange" data-index="{{activeIndex}}">
                  <label  wx:for="{{tabs[activeIndex].content}}"  wx:key="{{*this}}">
                    <view class="weui-navbar__item stop">
                    <checkbox class="weui-check" value="{{item.value}}" checked="{{item.checked}}"/>
                    <view class="weui-navbar__title stop_1 flaot_left">{{item.name}}</view>
                    <view class="flaot_right"  wx:if="{{item.checked}}">
                        <icon class="weui-icon-radio" type="success_no_circle"  color="#14BAC8" size="16"></icon>
                    </view>
                    </view>
                  </label>
                  </checkbox-group>
                </view>
                <!-- 类型  end -->

                <!-- 行业  start -->
                <view class="" hidden="{{activeIndex != 2}}">                
                  <checkbox-group bindchange="checkboxChange" data-index="{{activeIndex}}">
                  <label wx:for="{{tabs[activeIndex].content}}" wx:key="{{*this}}">
                    <view class="weui-navbar__item stop">
                    <checkbox class="weui-check" value="{{item.value}}" checked="{{item.checked}}"/>
                    <view class="weui-navbar__title stop_1 flaot_left">{{item.name}}</view>
                    <view class="flaot_right"  wx:if="{{item.checked}}">
                        <icon class="weui-icon-radio" type="success_no_circle"  color="#14BAC8" size="16"></icon>
                    </view>
                    </view>
                  </label>
                  </checkbox-group>
                </view>
                <!-- 行业  end -->
            </view>
            <view class="botton_view">
              <!-- 标签  start -->
              <view class="biaoqian">
                <!-- 金额  start -->
                <view  wx:for="{{str.AmountOfMoney}}" wx:key="{{*this}}">
                  <view class="delete" wx:if="{{item != ''}}">
                    <view class="delete_text">{{item.minThreshold}} - {{item.maxThreshold}} 万</view>
                    <image class="delete_image"  bindtap="clearBtn" data-name="AmountOfMoney" data-index="{{index}}"  data-value="{{item}}" src="/assets/images/delete.png"></image>
                  </view>
                </view>
                <!-- 金额  end -->
                
                <!-- 类型  start -->
                <view  wx:for="{{str.releaseTypeList}}" wx:key="{{*this}}">
                  <view class="delete" wx:if="{{item != ''}}">
                    <view class="delete_text">{{item.releaseType}}</view>
                    <image class="delete_image"  bindtap="clearBtn"  data-name="releaseTypeList" data-index="{{index}}" data-value="{{item.releaseType}}" src="/assets/images/delete.png"></image>
                  </view>
                </view>
                <!-- 类型  end -->

                <!-- 行业  start -->
                <view  wx:for="{{str.industryChoiceList}}" wx:key="{{*this}}">
                  <view class="delete" wx:if="{{item != ''}}">
                    <view class="delete_text">{{item.industryChoice}}</view>
                    <image class="delete_image"  bindtap="clearBtn" data-name="industryChoiceList" data-index="{{index}}" data-value="{{item.industryChoice}}" src="/assets/images/delete.png"></image>
                  </view>
                </view>
                <!-- 行业  end -->
              </view>
              <!-- 标签  end -->

              <!-- 操作  start -->
              <view class="operation">
                <view class="operation_close"  bindtap="bindtapReset">重置</view>
                <view class="operation_ok" bindtap="bindtapOk">确定</view>
              </view>
              <!-- 操作  end -->
            </view>
        </view>
    </view>
</view>
<!-- 筛选   end-->

<!-- 发布按钮  start-->
